{{outlet}}

<div class="row">
  <div class="col-sm-4">
    <label>Object ID</label>
    {{input type='number' value=object_id}}
  </div>

  <div class="col-sm-4">
    <label>Object Type</label>
  </div>

  <div class="col-sm-4">
    <label>Timestamp</label>
  </div>
</div>

<div class="row change_logs">
  <code class="col-sm-12">
    {{#each changes as |change| }}
      <label>{{change.key}}</label>:
      <p>{{change.value}}</p>
    {{/each}}
  </code>
</div>
